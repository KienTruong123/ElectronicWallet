<!doctype html>
<html lang="en">
  <head>
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!-- Bootstrap CSS -->
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.1/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-F3w7mX95PdgyTmZZMECAngseQB83DfGTowi0iMjiWaeVhAn4FJkqJByhZMI3AhiU" crossorigin="anonymous">



    <title>Dashboard</title>
  </head>
  <body>
    <div class="container">
      <nav class="navbar navbar-expand-lg navbar-light">
        <div class="container-fluid">
          <a class="navbar-brand" href="#">Home</a>
          <div class="navbar-toggler collapsed d-flex d-lg-none flex-column justify-content-around" type="button" data-bs-toggle="collapse" data-bs-target="#navbarNav" aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
            <div style="align-items: center;justify-content: center;border :0;border-color: transparent">
                <span class="toggler-icon top-bar"></span>
                <span class="toggler-icon middle-bar"></span>
                <span class="toggler-icon bottom-bar"></span>
            </div>
          </div>
          <div class="collapse navbar-collapse" id="navbarNav">
            <ul class="navbar-nav">
              <li class="nav-item">
                <a class="nav-link active" aria-current="page" href="#">List A</a>
              </li>
              <li class="nav-item">
                <a class="nav-link" href="#">List B</a>
              </li>
              <li class="nav-item">
                <a class="nav-link" href="#">Pricing</a>
              </li>
              <li class="nav-item">
                <a class="nav-link disabled" href="#" tabindex="-1" aria-disabled="true">Disabled</a>
              </li>
            </ul>
          </div>

        </div>
      </nav>
      <div class="d-flex justify-content-between">
        <h2 class="flex-grown">Hello, Admin</h2>
        <button id="btnLogOut" class="btn btn-danger float-right">Đăng xuất</button>
      </div>
    </div>
    
  
<!-- Tabs content -->
<div class="m-4">
    <ul class="nav nav-tabs" id="myTab">
        <li class="nav-item">
            <a href="#home" class="nav-link active" data-bs-toggle="tab"><i class="fa-solid fa-user-check" style="color: rgb(92, 208, 92)"></i> Đã kích hoạt</a>
        </li>
        <li class="nav-item">
            <a href="#profile" class="nav-link" data-bs-toggle="tab"><i class="fa-solid fa-user-plus" style="color: rgb(114, 182, 231)"></i> Chờ kích hoạt</a>
        </li>
        <li class="nav-item">
            <a href="#messages" class="nav-link" data-bs-toggle="tab"><i class="fa-solid fa-user-xmark" style="color: rgb(244, 83, 83)"></i> Vô hiệu hóa</a>
        </li>
        <li class="nav-item">
            <a href="#lock_users" class="nav-link" data-bs-toggle="tab"><i class="fa-solid fa-user-lock" style="color: rgb(254, 194, 53)"></i> Khóa vô thời hạn</a>
        </li>
        <li class="nav-item">
            <a href="#history_trade" class="nav-link" data-bs-toggle="tab"><i class="fa-solid fa-chart-line"></i> Lịch sử giao dịch chờ duyệt</a>
        </li>
    </ul>
    <div class="tab-content">
        <div class="tab-pane fade show active" id="home">
            <h4 class="my-3">Danh sách tài khoản đã kích hoạt</h4>
              <table class="table table-hover table-striped border">
                <thead>
                  <tr>
                    <th>ID</th>
                    <th>Name</th>
                    <th>Email</th>
                    <th>Created At</th>
                    <th>Status</th>
                  </tr>
                </thead>
                <tbody>
                   <tr style="cursor: pointer">
                    <td><a href="admin/users/123">294300f33fd</a></td>
                    <td>Nguyen Duc Trong</td>
                    <td>ductrong1313@gmail.com</td>
                    <td>12:00:21 PM 22/12</td>
                    <td>Active</td>
                  </tr>
                  {{#each active_list}}
                    <tr>
                        <td>
                            <a href="admin/users/{{_id}}">{{_id}}</a>
                        </td>
                        <td>{{{name}}}</td>
                        <td>{{email}}</td>
                        <td>{{createdAt}}</td>
                        <td>{{status}}</td>
                    </tr>
                {{/each}}
                </tbody>
              </table>
        </div>
        <div class="tab-pane fade" id="profile">
            <h4 class="my-3">Danh sách tài khoản chờ kích hoạt</h4>
            <table class="table table-hover table-striped border">
                <thead>
                  <tr>
                    <th>ID</th>
                    <th>Name</th>
                    <th>Email</th>
                    <th>Created At</th>
                    <th>Status</th>
                  </tr>
                </thead>
                 <tr style="cursor: pointer">
                    <td><a href="admin/users/123">294300f33fd</a></td>
                    <td>Nguyen Duc Trong</td>
                    <td>ductrong1313@gmail.com</td>
                    <td>12:00:21 PM 22/12</td>
                    <td>Active</td>
                  </tr>
                  {{#each waiting_list}}
                    <tr>
                        <td>
                            <a href="admin/users/{{_id}}">{{_id}}</a>
                        </td>
                        <td>{{name}}</td>
                        <td>{{email}}</td>
                        <td>{{createdAt}}</td>
                        <td>{{status}}</td>
                    </tr>
                {{/each}}
              </table>
        </div>
        <div class="tab-pane fade" id="messages">
            <h4 class="my-3">Danh sách tài khoản đang vô hiệu hóa</h4>
            <table class="table table-hover table-striped border">
                <thead>
                  <tr>
                    <th>ID</th>
                    <th>Name</th>
                    <th>Email</th>
                    <th>Created At</th>
                    <th>Status</th>
                  </tr>
                </thead>
                 <tr style="cursor: pointer">
                    <td><a href="admin/users/123">294300f33fd</a></td>
                    <td>Nguyen Duc Trong</td>
                    <td>ductrong1313@gmail.com</td>
                    <td>12:00:21 PM 22/12</td>
                    <td>Active</td>
                  </tr>
                  {{#each ban_list}}
                    <tr>
                        <td>
                           <a href="admin/users/{{_id}}">{{_id}}</a>
                        </td>
                        <td>{{name}}</td>
                        <td>{{email}}</td>
                        <td>{{createdAt}}</td>
                        <td>{{status}}</td>
                    </tr>
                {{/each}}
              </table>
        </div>
        <div class="tab-pane fade" id="lock_users">
            <h4 class="my-3">Danh sách tài khoản bị khóa vô thời hạn</h4>
            <table class="table table-hover table-striped border">
                <thead>
                  <tr>
                    <th>ID</th>
                    <th>Name</th>
                    <th>Email</th>
                    <th>Created At</th>
                    <th>Security Status</th>
                  </tr>
                </thead>
                 <tr style="cursor: pointer">
                    <td><a href="admin/users/123">294300f33fd</a></td>
                    <td>Nguyen Duc Trong</td>
                    <td>ductrong1313@gmail.com</td>
                    <td>12:00:21 PM 22/12</td>
                    <td>Security Status</td>
                  </tr>
                  {{#each lock_list}}
                    <tr>
                      
                        <td><a href="admin/users/{{_id}}">{{_id}}</a></td>
                        <td>{{name}}</td>
                        <td>{{email}}</td>
                        <td>{{lockedAt}}</td>
                        <td>Khóa vô thời hạn</td>
                    </tr>
                {{/each}}
              </table>
        </div>
        <div class="tab-pane fade" id="history_trade">
            <h4 class="my-3">Lịch sử giao dịch của người dùng trên 5 triệu đồng</h4>
            <table class="table table-hover table-striped border">
                <thead>
                  <tr>
                    <th>ID</th>
                    <th>Sender</th>
                    <th>Receiver</th>
                    <th>Amount</th>
                    <th>Created At</th>
                    <th>Status</th>
                  </tr>
                </thead>
                 <tr style="cursor: pointer">
                    <td><a href="admin/trades/123">1234</a></td>
                    <td><a href="admin/users/123">Nguyen Duc Trong</a></td>
                    <td><a href="admin/users/123">Tran Si Nguyen</a></td>
                    <td>200.000</td>
                    <td>12:00:21 PM 22/12</td>
                    <td>Active</td>
                  </tr>
                  {{#each trade_list}}
                    <tr>
                       <td><a href="admin/trades/{{_id}}">{{_id}}</a></td>
                        <td><a href="admin/users/{{sender_id}}">{{sender_name}}</a></td>
                    <td> <a href="admin/users/{{receiver_id}}">{{receiver_name}}</td>
                    <td>{{amount}}</td>
                    <td>{{createdAt}}</td>
                    <td>{{status}}</td>
                    </tr>
                {{/each}}
              </table>
        </div>
    </div>
</div>

  </body>
</html>